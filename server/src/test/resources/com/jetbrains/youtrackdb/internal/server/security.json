{
  "enabled": true,
  "debug": false,
  "server": {
    "createDefaultUsers": true
  },
  "authentication": {
    "enabled": true,
    "allowDefault": true,
    "authenticators": [
      {
        "name": "Password",
        "class": "com.jetbrains.youtrackdb.internal.core.security.authenticator.DefaultPasswordAuthenticator",
        "enabled": true,
        "users": [
          {
            "username": "guest",
            "resources": "server.listDatabases,server.dblist"
          }
        ]
      },
      {
        "name": "ServerConfig",
        "class": "com.jetbrains.youtrackdb.internal.server.security.authenticator.ServerConfigAuthenticator",
        "enabled": true
      },
      {
        "name": "SystemAuthenticator",
        "class": "com.jetbrains.youtrackdb.internal.server.security.authenticator.SystemUserAuthenticator",
        "enabled": true
      }
    ]
  },
  "auditing": {
    "class": "com.jetbrains.youtrackdb.internal.security.auditing.DefaultAuditing",
    "enabled": false
  }
}