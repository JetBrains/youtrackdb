<youtrackdb-server>
  <handlers>
    <!-- AUTOMATIC BACKUP, TO TURN ON SET THE 'ENABLED' PARAMETER TO 'true' -->
    <handler class="com.jetbrains.youtrackdb.internal.server.handler.AutomaticBackup">
      <parameters>
        <parameter name="enabled" value="false"/>
        <!-- LOCATION OF JSON CONFIGURATION FILE -->
        <parameter name="config" value="${YOUTRACKDB_HOME}/config/automatic-backup.json"/>
      </parameters>
    </handler>
    <handler class="com.jetbrains.youtrackdb.internal.server.plugin.gremlin.GremlinServerPlugin">
      <parameters>
        <parameter name="config" value="${YOUTRACKDB_HOME}/config/gremlin-server.yaml"/>
      </parameters>
    </handler>
  </handlers>
  <storages>
  </storages>
  <users>
  </users>
  <properties>
  </properties>
</youtrackdb-server>
